function plusMinus(arr) {

    var s=0;
    var a=0;
    var b=0;
    for(var i=0;i<arr.length;i++)
    {
        if(arr[i]>0)
        {
            s++;
        }
        if(arr[i]<0)
        {
            a++;
        }
        if(arr[i]==0)
        {
            b++;
        }
    }

    console.log(s/arr.length);
    console.log(a/arr.length);
    console.log(b/arr.length);

}
