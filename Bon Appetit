function bonAppetit(bill, k, b) {

    var sum =0;
    for(var i=0;i<bill.length;i++)
    {
        if(i!=k)
        {
            sum = sum + bill[i];
        }
    }
    var diff=0;

    diff = sum/2;

    if(diff == b)
    {
        console.log('Bon Appetit');
    }

    else
    {
        console.log(Math.abs(diff-b));
    }




}
