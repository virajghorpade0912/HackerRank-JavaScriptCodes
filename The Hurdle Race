function hurdleRace(k, height) {

    var max=0;
    var diff=0;
    
    for(var i=0;i<height.length;i++)
    {
        if(height[i]>max)
        {
            max=height[i];
        }
    }
    if(k<max)
    {
        diff = max-k;
        return diff;
    }
    else
    {
        return 0;
    }


}
